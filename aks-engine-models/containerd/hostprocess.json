{
    "apiVersion": "vlabs",
    "properties": {
      "orchestratorProfile": {
        "orchestratorType": "Kubernetes",
        "orchestratorRelease": "1.22",
        "kubernetesConfig": {
          "networkPlugin": "azure",
          "containerRuntime": "containerd",
          "windowsContainerdURL": "https://github.com/kubernetes-sigs/sig-windows-tools/releases/download/windows-containerd-nightly/windows-containerd.tar.gz",
          "useManagedIdentity": false,
          "kubeletConfig": {
              "-v": "5",
              "--feature-gates": "WindowsHostProcessContainers=true"
          },
          "apiServerConfig": {
            "--feature-gates": "WindowsHostProcessContainers=true"
          }
        }
      },
      "masterProfile": {
        "count": 1,
        "dnsPrefix": "",
        "vmSize": "Standard_D2_v2"
      },
      "agentPoolProfiles": [
        {
          "name": "linuxpool1",
          "count": 1,
          "vmSize": "Standard_D2_v2",
          "availabilityProfile": "VirtualMachineScaleSets"
        },
        {
          "name": "windowspool2",
          "count": 1,
          "vmSize": "Standard_D2s_v3",
          "osDiskSizeGB": 128,
          "availabilityProfile": "VirtualMachineScaleSets",
          "osType": "Windows"
        }
      ],
      "windowsProfile": {
        "adminUsername": "azureuser",
        "enableAutomaticUpdates": false,
        "sshEnabled": true,
        "windowsPublisher": "microsoft-aks",
        "windowsOffer": "aks-windows",
        "windowsSku": "2019-datacenter-core-ctrd-2011",
        "imageVersion": "latest"
      },
      "linuxProfile": {
        "adminUsername": "azureuser",
        "ssh": {
          "publicKeys": [
            {
              "keyData": "ssh-rsa  jstur@jstur-home"
            }
          ]
        }
      },
      "servicePrincipalProfile": {
        "clientId": "",
        "secret": ""
      }
    }
  }
  